{
    "name": "gmtls/openssl-crypto-kit",
    "description": "A modern and extensible PHP cryptography toolkit powered by OpenSSL.",
    "keywords": [
        "RSA",
        "EC",
        "encryption",
        "decryption",
        "signing",
        "verification",
        "OpenSSL"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/openssl-crypto-kit",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GmTLS\\CryptoKit\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
